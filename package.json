{
  "name": "fortnox-node-oauth-kit",
  "version": "0.1.0",
  "description": "A simple Node.js SDK for authenticating against Fortnox via OAuth 2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/server.ts",
    "clean": "rm -rf dist",
    "start": "node dist/server.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src/ --ext .ts",
    "lint:fix": "eslint src/ --ext .ts --fix"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "fortnox",
    "oauth",
    "api",
    "typescript",
    "sdk"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "pg": "^8.11.3",
    "qs": "^6.11.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.1",
    "@types/pg": "^8.10.9",
    "@types/qs": "^6.9.10",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.54.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2"
  }
}
